<include src='/include/change_city.wxml' />

<block wx:for="{{arrays}}">
  <view class='item' bindtap='goSecenery' data-name='{{item.name}}' data-id='{{item.id}}'>
    <image src='{{item.img}}' mode='aspectFit' class='picture'/>
    <view>
        <view class='name'>{{item.name}}</view>
        <include src='/include/star_item.wxml' />
        <view class='count'>
            <view>{{item.town}}</view>
            <view style='position: relative;left: 20px'>{{item.route}}</view>
        </view>
        <view class='intro'>{{item.intro}}</view>
    </view>
    <view>
    </view>
    <image src='/icon/array.png' mode='aspectFit' class='array'/>
  </view>
</block>
